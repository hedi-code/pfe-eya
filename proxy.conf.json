{
  "/jenkins": {
    "target": "http://localhost:8080",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/jenkins": ""
    },
    "onProxyReq": function(proxyReq, req, res) {
      const auth = Buffer.from('admin:eyaelouni').toString('base64');
      proxyReq.setHeader('Authorization', 'Basic ' + auth);
    }
  }
}
